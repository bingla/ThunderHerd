{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RunServiceOptions": {
    "MaxParallelThreads": 4,
    "MaxCallsPerSecond": 20,
    "MaxRunDurationInSeconds": 3600,
    "MaxWarmupDurationInSeconds": 3600,
    "TimeSlotSpanInSeconds": 1
  },
  "AllowedHosts": "*"
}